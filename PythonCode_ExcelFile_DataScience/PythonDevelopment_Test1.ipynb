        "# function to set the date format to a user entered type\n",
        "\n",
        "def userdateformat(df, colname):\n",
        "  dateformat = input('Please enter the date format:\\n')\n",
        "  dateformat = dateformat[:].lower()\n",
        "  dateformat = dateformat.replace('dd', '%d').replace('mm', '%m')\n",
        "  dateformat = dateformat.replace('yyyy', '%Y')\n",
        "  datecol_val = pd.to_datetime(df[colname],format = '%Y-%m-%d')\n",
        "  userformat = pd.Series(datecol_val.dt.strftime(dateformat))\n",
        "  df['userformatdate'] = userformat\n",
        "  return df\n"
      ],
      "execution_count": null,
      "outputs": []
    },
