      "source": [
        "# function to identify anomalies in such as string in an integer columns\n",
        "import re\n",
        "def remove_anomalies(df):\n",
        "  specialchr = re.compile('[@_!#$%^&*()<>?/\\|}{~:]')\n",
        "  for col in df.select_dtypes(['object']).columns.tolist():\n",
        "    if df[col].apply(lambda x : (str(x).isnumeric() or str(x).isalnum() or (specialchr.search(str(x)) != None))).all():\n",
        "      newlst = list(map(float, df[col].str.extract(r'([0-9]+)', expand=False)))\n",
        "      oldlst = list(df[col])\n",
        "      df[col] = df[col].replace(oldlst, newlst)\n",
        "  return df"
      ],
      "execution_count": null,
      "outputs": []
    },
