      "source": [
        "# function to identify outliers and clean them\n",
        "\n",
        "def removeoutliers(df):\n",
        " # Interquartile Range (IQR)\n",
        "  datetimecols = df.select_dtypes(['datetime64']).columns.tolist()\n",
        "  objectcols = df.select_dtypes(['object']).columns.tolist()\n",
        "  numbercols = df.select_dtypes(['number']).columns.tolist()\n",
        "  temp = df.filter(numbercols, axis = 1)\n",
        "  tempobj = df.filter(objectcols + datetimecols,  axis = 1)\n",
        "  Q1 = temp.quantile(0.25)\n",
        "  Q3 = temp.quantile(0.75)\n",
        "  IQR = Q3 - Q1\n",
        "  temp = temp[((temp >= (Q1 - 1.5 * IQR)) & (temp <= (Q3 + 1.5 * IQR))).all(axis=1)]\n",
        "  temp = pd.concat([temp, tempobj], axis = 1) \n",
        "  return temp"
      ],
      "execution_count": null,
      "outputs": []
    },
