      "source": [
        "# function to generate summary stats of the data and save it in a file\n",
        "\n",
        "def stats_summary(df):\n",
        "  statssummary = {}\n",
        "  numbercols = df.select_dtypes(['number']).columns.tolist()\n",
        "  for col in numbercols:\n",
        "    templst = []\n",
        "    templst.append(df[col].count())\n",
        "    templst.append(df[col].mean())\n",
        "    templst.append(df[col].std())\n",
        "    templst.append(df[col].min())\n",
        "    templst.append(df[col].quantile(0.25))\n",
        "    templst.append(df[col].quantile(0.50))\n",
        "    templst.append(df[col].quantile(0.75))\n",
        "    templst.append(df[col].max())\n",
        "    statssummary[col] = templst\n",
        "  stats_summarydf = pd.DataFrame(statssummary, columns = numbercols)\n",
        "  from google.colab import files\n",
        "  stats_summarydf.to_excel('Summary_Stats.xlsx')         \n",
        "  files.download('Summary_Stats.xlsx')"
      ],
      "execution_count": null,
      "outputs": []
    },
